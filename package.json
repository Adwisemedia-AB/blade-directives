{
  "name": "sage-directives",
  "version": "1.0.0",
  "author": "Brandon Nifong <brandon@tendency.me>",
  "homepage": "https://github.com/log1x/sage-directives",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/log1x/sage-directives.git"
  },
  "bugs": {
    "url": "https://github.com/log1x/sage-directives/issues"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && docpress b",
    "build:production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && docpress b",
    "deploy": "yarn run build:production && git-update-ghpages -b master log1x/sage-directives-docs _docpress",
    "start": "NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "dependencies": {
    "laravel-mix": "^4.0.14"
  },
  "devDependencies": {
    "docpress": "^0.7.6",
    "git-update-ghpages": "^1.3.0",
    "resolve-url-loader": "2.3.1",
    "sass": "^1.17.2",
    "sass-loader": "7.*"
  }
}
